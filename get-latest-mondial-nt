docker run -e VIS_MONDIAL_USER \
-e VIS_MONDIAL_PASSWORD \
-e VIS_MONDIAL_HOST \
-e VIS_MONDIAL_PORT \
-e VIS_MONDIAL_SSLMODE \
-e VIS_MONDIAL_DBNAME \
 dooodle/vis-extractor

docker run -e VIS_MONDIAL_USER=$VIS_MONDIAL_USER \
-e VIS_MONDIAL_PASSWORD=$MONDIAL_PASSWORD \
-e VIS_MONDIAL_HOST=$MONDIAL_HOST \
-e VIS_MONDIAL_PORT=$MONDIAL_PORT \
-e VIS_MONDIAL_SSLMODE=$MONDIAL_SSLMODE \
-e VIS_MONDIAL_DBNAME=$MONDIAL_DBNAME \
 dooodle/vis-extractor  > jena-fuseki/mondial.nt
